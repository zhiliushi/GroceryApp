{
  "name": "GroceryApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest --ci --forceExit --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --ci --forceExit --passWithNoTests",
    "test:unit": "jest --testPathPattern='utils/|repositories/' --ci --forceExit",
    "test:integration": "jest --testPathPattern='integration/' --ci --forceExit",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "validate": "npm run type-check && npm run lint && npm run test",
    "build:android": "cd android && .\\gradlew.bat assembleRelease && cd ..",
    "build:android:debug": "cd android && .\\gradlew.bat assembleDebug && cd ..",
    "build:ios": "cd ios && xcodebuild -workspace GroceryApp.xcworkspace -scheme GroceryApp -configuration Release -sdk iphoneos archive && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "clean": "cd android && .\\gradlew.bat clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "clean:all": "rm -rf node_modules && npm install",
    "clean:cache": "react-native start --reset-cache"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@notifee/react-native": "^9.1.8",
    "@nozbe/watermelondb": "^0.28.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/analytics": "^23.8.4",
    "@react-native-firebase/app": "^23.8.4",
    "@react-native-firebase/auth": "^23.8.4",
    "@react-native-firebase/crashlytics": "^23.8.4",
    "@react-native-firebase/firestore": "^23.8.4",
    "@react-native-firebase/storage": "^23.8.4",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "@react-native/new-app-screen": "0.83.1",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.11.0",
    "@react-navigation/stack": "^7.6.16",
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.4",
    "date-fns": "^4.1.0",
    "react": "19.2.0",
    "react-hook-form": "^7.71.1",
    "react-native": "0.83.1",
    "react-native-background-fetch": "^4.3.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.20.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-vision-camera": "^4.7.3",
    "uuid": "^11.0.4",
    "zod": "^3.24.2",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterSetup": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@screens/(.*)$": "<rootDir>/src/screens/$1",
      "^@hooks/(.*)$": "<rootDir>/src/hooks/$1",
      "^@store/(.*)$": "<rootDir>/src/store/$1",
      "^@database/(.*)$": "<rootDir>/src/database/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation|react-native-vector-icons|react-native-sqlite-storage|@nozbe|react-native-paper|react-native-safe-area-context|@notifee|react-native-background-fetch)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts",
      "!src/types/**"
    ]
  }
}
